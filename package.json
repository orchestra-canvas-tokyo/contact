		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.15.0",
		"prettier": "^3.5.1",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.20.2",
		"svelte-check": "^4.1.4",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.24.1",
		"vite": "^6.1.1"
	},
	"dependencies": {
		"husky": "^9.1.7",
		"npm-run-all": "^4.1.5"
